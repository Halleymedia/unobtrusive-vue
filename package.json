{
  "name": "@halleymedia/unobtrusive-vue",
  "description": "A thin abstraction layer over the Vue.js renderer for unobtrusive use",
  "version": "1.4.0",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Halleymedia",
    "email": "dev@halleymedia.com"
  },
  "bugs": {
    "email": "dev@halleymedia.com"
  },
  "homepage": "https://github.com/Halleymedia/unobtrusive-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/Halleymedia/unobtrusive-vue.git"
  },
  "keywords": [
    "unobtrusive",
    "rendering",
    "vue",
    "frameworkless"
  ],
  "main": "lib/index.js",
  "scripts": {
    "serve": "webpack-dev-server --mode development --config ./sample/webpack.config.js --inline",
    "build": "babel src --out-dir lib && webpack --config ./sample/webpack.config.js --mode production --devtool false --display-error-details",
    "publish": "npm run build && npm publish --access public",
    "test": "jest"
  },
  "dependencies": {
    "semistandard": "^14.2.3",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@types/webpack-env": "^1.15.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.0",
    "copyfiles": "^2.4.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "html-inline-css-webpack-plugin": "^1.9.0",
    "html-loader": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.5.2",
    "mini-css-extract-plugin": "^0.10.1",
    "mini-svg-data-uri": "^1.2.3",
    "sass": "^1.26.12",
    "sass-loader": "^10.0.2",
    "standard": "^14.3.4",
    "standard-loader": "^7.0.0",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ]
  }
}
